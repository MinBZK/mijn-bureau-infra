{{- if .Values.application.ollama.enabled }}
ai:
  llm:
    {{- if not .Values.ai.llm.model }}
    model: {{ .Values.application.ollama.model | quote }}
    {{- end }}
    {{- if not .Values.ai.llm.endpoint.host }}
    endpoint:
      host: ollama
      port: 11434
      openApiVersion: "v1"
      isSsl: false
      isInternal: true
    {{- end }}
    {{- if not .Values.ai.llm.apiKey }}
    apiKey: ""
    {{- end }}
{{- end }}
